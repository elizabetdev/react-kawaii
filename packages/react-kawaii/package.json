{
  "name": "react-kawaii",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "version": "1.3.0",
  "homepage": "https://github.com/miukimiu/react-kawaii#readme",
  "scripts": {
    "lint": "eslint ./src",
    "test": "jest",
