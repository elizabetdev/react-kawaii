  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@swc/core": "^1.5.7",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "change-case": "^5.4.3",
    "eslint-config-react-kawaii": "workspace:*",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
